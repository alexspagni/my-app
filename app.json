{
    "expo": {
        "name": "SpaceApplication",
        "slug": "NasaApiProjects",
        "version": "1.0.0",
        "orientation": "portrait",
        "icon": "./assets/spaceIcon.png",
        "splash": {
            "image": "./assets/splash.png",
            "resizeMode": "contain",
            "backgroundColor": "#ffffff"
        },
        "updates": {
            "fallbackToCacheTimeout": 0
        },
        "assetBundlePatterns": [
            "**/*"
        ],
        "ios": {
            "supportsTablet": true
        },
        "android": {
            "adaptiveIcon": {
                "foregroundImage": "./assets/spaceIcon.png",
                "backgroundColor": "#FFFFFF"
            },
            "package": "com.nasaApi.project"
        },
        "web": {
            "favicon": "./assets/favicon.png"
        }
    }
}